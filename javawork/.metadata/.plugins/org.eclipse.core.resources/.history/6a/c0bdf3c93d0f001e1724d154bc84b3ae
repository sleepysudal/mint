package day0620;

public class ForTest_16 {

	public static void main(String[] args) {
		//1. 1~5까지 출력하시오
		for(int i=1; i<=5; i++)
		{		
			System.out.print(i+" ");

		}
		System.out.println();

		for(int i=5; i>=1; i--) {
			System.out.print(i+" ");
		}
		System.out.println();
		
		//0,2,4,6,8,10
		for(int i=0; i<=10; i+=2) {
			System.out.println(i+" ");
		}
	}
}
