package day0616;

public class PrinfTest_06 {

	public static void main(String[] args) {
		//printf로 출력하세요
		//%d 정수 \n줄바꿈 %s가 문자 %f실수
		//안녕하세요 제이름은 송혜교입니다
		//나이는 25세입니다
		String name="송혜교";
		int age=25;
		double a=0.1;
		
		System.out.printf("안녕하세요 제이름은 %s입니다\n나이는 %d세입니다\n시력은%f입니다,name,age,a);
		
	}

}
