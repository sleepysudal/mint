package gittest;
import java.util.Scanner;
public class AplusB_02 {

	public static void main(String[] args) {
		//임포트
		Scanner sc= new Scanner(System.in);
		
		//변수지정
		int a,b,t;
		
		//값입력
		System.out.println("몇번 더할건지");
		t=sc.nextInt();
		
		System.out.println("두 정수 a,b 값 입력해라");
		a=sc.nextInt();
		b=sc.nextInt();
	}

}
